<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhangqie.pictureoperation">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.HdImgActivity"/>
        <activity android:name=".ui.TounImageViewActivity"/>
        <activity android:name=".ui.ZoomImageActivity"/>
        <activity android:name=".ui.ViewPagerImageViewZQUI"/>

        <activity android:name=".photoview.LauncherActivity"/>
        <activity android:name=".photoview.SimpleSampleActivity" />

        <activity android:name=".photoview.ViewPagerActivity" />

        <activity android:name=".photoview.RotationSampleActivity" />

        <activity android:name=".photoview.PicassoSampleActivity" />

        <activity android:name=".photoview.ActivityTransitionActivity" />

        <activity android:name=".photoview.ActivityTransitionToActivity" />

        <activity android:name=".photoview.ImmersiveActivity" />
    </application>

</manifest>